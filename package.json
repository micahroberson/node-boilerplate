    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "jest-cli": "^15.1.1",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "react-addons-test-utils": "^15.3.2",
    "sinon": "^1.17.6",
    "supertest": "^2.0.0",

{
  "name": "node-boilerplate",
  "version": "0.0.1",
  "description": "A Node boilerplate app with basic auth support built with React, Postgres, Express, etc.",
  "scripts": {
    "test": "make test"
  },
  "jest": {
    "automock": true,
    "testPathDirs": [
      "<rootDir>/src/common"
    ],
    "testRegex": ".*\\.test\\.js$",
    "unmockedModulePathPatterns": [
      "aphrodite",
      "bluebird",
      "fluxible",
      "react",
      "react-dom",
      "react-addons-test-utils",
      "lodash",
      "cheerio",
      "enzyme",
      "sinon"
    ]
  },
  "author": "Micah Roberson",
  "repository": {
    "type": "git",
    "url": "https://github.com/micahroberson/node-boilerplate.git"
  },
  "license": "MIT",
  "dependencies": {
    "aphrodite": "^0.5.0",
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "bson-objectid": "^1.1.4",
    "bull": "^1.0.0",
    "cookie-dough": "^0.1.0",
    "cookie-parser": "^1.4.3",
    "db-migrate": "^0.10.0-beta.16",
    "db-migrate-pg": "^0.1.10",
    "express": "^4.14.0",
    "express-useragent": "^1.0.4",
    "file-loader": "^0.9.0",
    "fluxible": "^1.2.0",
    "fluxible-addons-react": "^0.2.8",
    "history": "^4.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.15.0",
    "mandrill-api": "^1.0.45",
    "moment": "^2.15.2",
    "node-postgres-named": "^2.4.0",
    "pg": "^6.1.0",
    "pg-native": "^1.10.0",
    "pm2": "^2.0.15",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-portal": "^2.2.1",
    "react-router": "^2.8.1",
    "serialize-javascript": "^1.3.0",
    "stripe": "^4.11.0",
